#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int result =0;
    for(int i=0;i<n;i++){
        result ^= a[i];
    }
    printf("%d\n",result);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
